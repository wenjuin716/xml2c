<?xml version="1.0" ?>
<xmlMandatorySingleRootNode>

<object name="Device." shortObjectName="Dev" specSource="TR181" profile="Baseline:1" supportLevel="Present" />
  <description source="TRx">The top-level object for an TR181. </description>

  <parameter name="RootDataModelVersion" type="string" specSource="TR181" profile="Baseline:1" supportLevel="ReadOnly" maxLength="32" />
    <description source="TRx">Root data model version. </description>

  <parameter name="InterfaceStackNumberOfEntries" type="unsignedInt" specSource="TR181" profile="Baseline:1" supportLevel="ReadOnly" alwaysWriteToConfigFile="true" />
    <description source="TRx">The number of entries in the InterfaceStack table. </description>



<object name="Device.Ethernet." shortObjectName="DevEth" specSource="TR181" profile="Baseline:1" supportLevel="Present" />
  <description source="TRx">This object models several Ethernet interface objects. </description>

  <parameter name="InterfaceNumberOfEntries" type="unsignedInt" specSource="TR181" profile="Baseline:1" supportLevel="ReadOnly" />
    <description source="TRx">The number of entries in the Interface table. </description>



<object name="Device.Ethernet.Interface.{i}." shortObjectName="DevEthIntf" specSource="TR181" profile="Baseline:1" supportLevel="MultipleInstances" />
  <description source="TRx">Ethernet interface table. </description>

  <parameter name="Enable" type="boolean" specSource="TR181" profile="Baseline:1" supportLevel="ReadWrite" defaultValue="true" />

  <parameter name="Status" type="string" specSource="TR181" profile="Baseline:1" supportLevel="ReadOnly" validValuesArray="EthIntStateValues" />
    <description source="TRx">Number of instances of LANEthernetInterface-&#10;Config in this LANDevice. </description>



<object name="Device.Ethernet.Link.{i}." shortObjectName="DevEthLink" specSource="TR181" profile="Baseline:1" supportLevel="MultipleInstances" />
  <description source="TRx">Ethernet link layer table (a stackable interface object as described in [Section 4.2/TR-181i2]).  </description>

  <parameter name="Enable" type="boolean" specSource="TR181" profile="Baseline:1" supportLevel="ReadWrite" defaultValue="true" />

  <parameter name="Status" type="string" specSource="TR181" profile="Baseline:1" supportLevel="ReadOnly" validValuesArray="EthIntStateValues" />
    <description source="TRx">The current operational state of the link (see [Section 4.2.2/TR-181i2]). </description>
	


<object name="Device.WiFi." shortObjectName="DevWiFi" specSource="TR181" profile="Baseline:1" supportLevel="Present" />
  <description source="TRx">The WiFi object is based on the WiFi Alliance 802.11 specifications ([802.11-2007]).  </description>

  <parameter name="RadioNumberOfEntries" type="unsignedInt" specSource="TR181" profile="Baseline:1" supportLevel="ReadOnly" />
    <description source="TRx">The number of entries in the Radio table. </description>



<object name="Device.WiFi.Radio.{i}." shortObjectName="DevWiFiRadio" specSource="TR181" profile="Baseline:1" supportLevel="MultipleInstances" />
  <description source="TRx">This object models an 802.11 wireless radio on a device (a stackable interface object as described in [Section 4.2/TR-181i2]). </description>

  <parameter name="Enable" type="boolean" specSource="TR181" profile="Baseline:1" supportLevel="ReadWrite" defaultValue="true" />

  <parameter name="Status" type="string" specSource="TR181" profile="Baseline:1" supportLevel="ReadOnly" validValuesArray="WiFiRadioStateValues" />
    <description source="TRx">The current operational state of the radio (see [Section 4.2.2/TR-181i2]). </description>



<validstringarray name="EthIntStateValues" />
  <element>Up</element>
  <element>Down</element>
  <element>Unknown</element>
  <element>Dormant</element>
  <element>NotPresent</element>
  <element>LowerLayerDown</element>
  <element>Error</element>



<validstringarray name="WiFiRadioStateValues" />
  <element>Up</element>
  <element>Down</element>
  <element>Unknown</element>
  <element>Dormant</element>
  <element>NotPresent</element>
  <element>LowerLayerDown</element>
  <element>Error</element>



</xmlMandatorySingleRootNode>
